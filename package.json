{
  "name": "@eatonfyi/urls",
  "version": "1.0.0",
  "description": "Extended parsing, normalization, and resolution of URLs.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "pkgroll",
    "test": "ava"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--import=tsimp"
    ]
  },
  "keywords": [
    "asin",
    "amazon",
    "scraper"
  ],
  "author": "eaton",
  "license": "MIT",
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@types/node": "^20.11.30",
    "ava": "^6.1.2",
    "pkgroll": "^2.0.2",
    "shx": "^0.3.4",
    "tsimp": "^2.0.11",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "tldts": "^6.1.14",
    "urlpattern-polyfill": "^10.0.0"
  }
}
